  15+ 9D92              ;                       Los bytes 1 y 2 definen la velocidad del desplazamiento.
  16+ 9D92              ;
  17+ 9D92              ;                       (Byte1) ..... % (Vel_up),(Vel_down)
  18+ 9D92              ;                               ..... %01000010 Vel_up 4
  19+ 9D92              ;                                               Vel_down 2
  20+ 9D92              ;                               ..... $42
  21+ 9D92              ;
  22+ 9D92              ;                       (Byte2) ..... % (Vel_left),(Vel_right)
  23+ 9D92              ;
  24+ 9D92              ;                               ..... %01000010 Vel_left 4
  25+ 9D92              ;                                               Vel_right 2
  26+ 9D92              ;                               ..... $42
  27+ 9D92              ;
  28+ 9D92              ;                       El 3er byte describe la dirección y repeticiones del desplazamiento.
  29+ 9D92              ;
  30+ 9D92              ;                       (Byte3) ..... Descripción del desplazamiento.
  31+ 9D92              ;
  32+ 9D92              ;                       % up,down,left,right, Repetición del desplazamiento, (1-15).
  33+ 9D92              ;
  34+ 9D92              ;                                     %01010011 ..... (Abajo-derecha), 3 veces.
  35+ 9D92              ;                                     $53
  36+ 9D92              ;
  37+ 9D92              ;                       Nota: (Repetimos_desplazamiento) nunca será "0".
  38+ 9D92              ;
  39+ 9D92              ;                       Esta tabla nos ayudará a codificar rápido los desplazamientos del mov.
  40+ 9D92              ;                       Supongamos que solo ejecutamos 1 vez, cada desplazamiento:
  41+ 9D92              ;
  42+ 9D92              ;                       Arriba ..... $81
  43+ 9D92              ;                       Arriba - izquierda ..... $a1
  44+ 9D92              ;                       Arriba - derecha ..... $91
  45+ 9D92              ;
  46+ 9D92              ;                       Abajo ..... $41
  47+ 9D92              ;                       Abajo - izquierda ..... $61
  48+ 9D92              ;                       Abajo - derecha ..... $51
  49+ 9D92              ;
  50+ 9D92              ;   (Cola_de_desplazamiento) db 0			; Este byte indica:
  51+ 9D92              ;
  52+ 9D92              ;											;	"$00" , "0" ..... Hemos finalizado la cadena de movimiento.
  53+ 9D92              ;											;				En este caso hemos de incrementar (Puntero_indice_mov)_
  54+ 9D92              ;											;				_ y pasar a la siguiente cadena de movimiento del índice.
  55+ 9D92              ;
  56+ 9D92              ;											;	"$fd" , "1-253" ..... Repetición del MOVIMIENTO.
  57+ 9D92              ;											;						         Nº de veces que vamos a repetir el movimiento completo.
  58+ 9D92              ;											;					             En este caso, volveremos a inicializar (Puntero_mov),_
  59+ 9D92              ;							    			;						         _ con (Puntero_indice_mov) y decrementaremos (Cola_de_desplazamiento).
